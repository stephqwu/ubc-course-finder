{
    "query1": {
        "WHERE": {
            "IS": {
                "courses_dept": "cpsc"
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_avg",
                "courses_id"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "courses_avg"
                ]
            }
        }
    },
    "query2": {
        "WHERE": {
            "GT": {
                "courses_avg": 97
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "courses_avg"
            ],
            "ORDER": {
                "dir": "UP",
                "keys": ["courses_avg"]
            }
        }
    }
}
